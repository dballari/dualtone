	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"text": "var(--wp--preset--color--base)"
				},
				"css": "border-radius: 0px; background-color: var(--wp--preset--color--contrast);"
			},
			"core/buttons": {
				"css": " & .wp-block-button__link {border-radius: 0px; background-color: var(--wp--preset--color--contrast);} & .is-style-accent .wp-block-button__link {background-color: var(--wp--preset--color--accent);}"
			},
			"core/cover": {
				"css": " &.is-style-variable-min-height {min-height: clamp(28.75rem, 43.913rem + -19.5652vw, 40rem);}"
			},
			"core/file": {
				"css": "& .wp-block-file__button.wp-element-button { background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 0; text-decoration: none; } & .wp-block-file__button.wp-element-button:hover { text-decoration: underline; }"
			},
			"core/group": {
				"css": " & .is-nowrap.is-layout-flex.is-style-baseline { align-items: baseline !important; } & .is-style-palette { width: 160px; height: 160px; padding-top: 2rem;} & .is-style-vborder { gap: 0; border-top: 1px solid var(--wp--preset--color--accent); border-bottom: 1px solid var(--wp--preset--color--accent); padding: var(--wp--preset--spacing--025) 0; }"
			},
			"core/image": {
				"variations": {
					"rounded": {
						"border": {
							"radius": "var(--wp--custom--gap--normal)"
						}
					}
				}
			},
			"core/latest-posts": {
				"css": " & li:not(:first-of-type) { padding-top: var(--wp--preset--spacing--10); }"
			},
			"core/navigation": {
				"css": "& button {border: 1px solid; padding: 4px;}",
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--bolder)"
				}
			},
			"core/post-content": {
				"css": " & a:not(.wp-element-button, [rel=\"tag\"]) { text-decoration: underline;} & a:link { color: var(--wp--preset--color--accent); } & a:visited { color: var(--wp--preset--color--accent-2); } & a.visited { color: var(--wp--preset--color--accent-2) !important; } & a.unvisited { color: var(--wp--preset--color--accent); }"
			},
			"core/post-excerpt": {
				"css": " & .wp-block-post-excerpt__more-link {font-weight: var(--wp--custom--font-weight--bolder);} & .wp-block-post-excerpt__more-link:hover {text-decoration: none;}"
			},
			"core/post-terms": {
				"css": " & a { padding: 4px; background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 4px; } & a:visited { color: var(--wp--preset--color--base); }"
			},
			"core/post-title": {
				"css": " & a:visited { color: var(--wp--preset--color--accent-2); }"
			},
			"core/query-pagination-next": {
				"css": "background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 4px; padding: var(--wp--preset--spacing--025);"
			},
			"core/query-pagination-numbers": {
				"css": " & a { background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 4px; padding: 7px; }"
			},
			"core/query-pagination-previous": {
				"css": "background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 4px; padding: var(--wp--preset--spacing--025);"
			},
			"core/search": {
				"css": " & .wp-block-search__input {font-family: var(--wp--preset--font-family--inter); font-size: var(--wp--preset--font-size--medium); border-color: var(--wp--preset--color--contrast); border-width: var(--wp--custom--border--thin); padding: var(--wp--preset--spacing--050); }; & .wp-block-search__input::placeholder {color: var(--wp--preset--color--contrast); opacity: 1; font-family: var(--wp--preset--font-family--inter); font-size: var(--wp--preset--font-size--medium); }; & :where(.wp-block-search__button-inside .wp-block-search__inside-wrapper) { padding: 0; border-color: var(--wp--preset--color--contrast);} & .wp-block-search__button {border: 0; background-color: var(--wp--preset--color--contrast);}"
			},
			"core/separator": {
				"css": "border: 2px solid; color: var(--wp--preset--color--accent); &.is-style-dots:before {content: 'Â·';}"
			},
			"core/table": {
				"css": "& th { word-break: initial; } & td { word-break: initial; & min-width: var(--wp--custom--width--min-table-width); & overflow-x: scroll;}"
			},
			"core/term-description": {
				"css": "& p {margin: 0 auto 0 0 !important; max-width: 75% !important;}"
			}
		},
		"color": {
