    "styles": {
        "blocks": {
            "core/site-logo": {
                "css": " & svg.custom-logo { width: var(--wp--custom--width--logo-width); height: var(--wp--custom--width--logo-width); }"
            },
            "core/button": {
                "color": {
                    "text": "var(--wp--preset--color--base)"
                },
                "css": "border-radius: 0px; background-color: var(--wp--preset--color--contrast);"
            },
            "core/buttons": {
                "css": " & .wp-block-button__link {border-radius: 0px; background-color: var(--wp--preset--color--contrast);} & .is-style-accent .wp-block-button__link {background-color: var(--wp--preset--color--accent); & .is-style-dark .wp-block-button__link {background-color: var(--wp--preset--color--contrast); }"
            },
            "core/cover": {
                "css": " &.is-style-variable-min-height {min-height: clamp(28.75rem, 43.913rem + -19.5652vw, 40rem);}"
            },
            "core/file": {
                "css": "& .wp-block-file__button.wp-element-button { background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 0; text-decoration: none; } & .wp-block-file__button.wp-element-button:hover { text-decoration: underline; opacity: 1; }"
            },
            "core/group": {
                "css": " & .is-style-vborder { gap: 0; border-top: 1px solid var(--wp--preset--color--accent); border-bottom: 1px solid var(--wp--preset--color--accent); padding: var(--wp--preset--spacing--025) 0; }"
            },
            "core/image": {
                "variations": {
                    "rounded": {
                        "border": {
                            "radius": "var(--wp--custom--gap--normal)"
                        }
                    }
                }
            },
            "core/latest-posts": {
                "css": " & li:not(:first-of-type) { padding-top: var(--wp--preset--spacing--10); }"
            },
            "core/navigation": {
                "css": "& button {border: 1px solid; padding: 4px;}",
                "typography": {
                    "fontWeight": "var(--wp--custom--font-weight--bolder)"
                }
            },
            "core/post-content": {
                "css": " & a:not(.wp-element-button, [rel=\"tag\"]) { text-decoration: underline;} & a:link:not([rel=\"tag\"],  .wp-block-file__button) { color: var(--wp--preset--color--accent); } & a:visited { color: var(--wp--preset--color--accent-2); } & a.visited { color: var(--wp--preset--color--accent-2) !important; } & a.unvisited { color: var(--wp--preset--color--accent); }"
            },
            "core/post-excerpt": {
                "css": " & .wp-block-post-excerpt__more-link {font-weight: var(--wp--custom--font-weight--bolder);} & .wp-block-post-excerpt__more-link:hover {text-decoration: none;} & .wp-block-post-excerpt__more-link:visited { color: var(--wp--preset--color--accent-2); }"
            },
            "core/post-template": {
              "css": " & .sticky .wp-block-post-title a::after { content: \"*\"}"  
            },
            "core/post-terms": {
                "css": " & a { padding: 4px; background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base); border-radius: 4px; } & a:visited { color: var(--wp--preset--color--base); }"
            },
            "core/query": {
                "css": " & a:not(.wp-block-query-pagination-next) { color: var(--wp--preset--color--accent); } & a:visited { color: var(--wp--preset--color--accent-2); }"
            },
            "core/query-pagination-next": {
                "css": " & background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base) !important; border-radius: 4px; padding: var(--wp--preset--spacing--025);"
            },
            "core/query-pagination-numbers": {
                "css": " & a { background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base) !important; border-radius: 4px; padding: 7px; }"
            },
            "core/query-pagination-previous": {
                "css": "background-color: var(--wp--preset--color--contrast); color: var(--wp--preset--color--base) !important; border-radius: 4px; padding: var(--wp--preset--spacing--025);"
            },
            "core/site-title": {
                "css": " & a { color: var(--wp--preset--color--contrast); }"
            },
            "core/separator": {
                "css": "border: 2px solid; color: var(--wp--preset--color--accent); &.is-style-dots:before {content: '·';}"
            },
            "core/table": {
                "css": "& th { word-break: initial; } & td { word-break: initial; & min-width: var(--wp--custom--width--min-table-width); & overflow-x: scroll;}"
            },
            "core/term-description": {
                "css": "& p {margin: 0 auto 0 0 !important; max-width: 75% !important;}"
            }
        },
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "css": "html {scroll-behavior: smooth;} figcaption { text-align: center;} figcaption:not(.blocks-gallery-caption) { padding-top: var(--wp--preset--spacing--050);} ",
        "elements": {
            "button": {
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                }
            },
            "caption": {
                "typography": {
