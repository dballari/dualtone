            "fontWeight": {
                "black": "900",
                "bold": "450",
                "bolder": "680",
                "default": "280",
                "semiBlack": "800"
            },
            "gap": {
                "normal": "1rem"
            },
            "lineHeight": {
                "huge": "1.111",
                "large": "1.495",
                "medium": "1.556",
                "small": "1.586",
                "x-large": "1.257",
                "xx-large": "1.189"
            },
            "spacing": {
                "normal": "1rem"
            },
            "width": {
                "contentFlexBasis": "100%",
                "contentSize": "42.8125rem",
                "maxWidth": "120rem",
                "sidebarFlexBasis": "444px",
                "sidebarSize": "20rem",
                "wideSize": "70rem",
