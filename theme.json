	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--contrast)"
		},
		"blocks": {
			"core/button": {
				"color": {
					"text": "var(--wp--preset--color--base)"
				},
				"css": "border-radius: 0px; background-color: var(--wp--preset--color--contrast);"
			},
			"core/buttons": {
				"css": " & .wp-block-button__link {border-radius: 0px; background-color: var(--wp--preset--color--contrast);} & .is-style-contrast-2 .wp-block-button__link {background-color: var(--wp--preset--color--contrast-2);}"
			},
			"core/cover": {
				"css": " &.is-style-variable-min-height {min-height: clamp(28.75rem, 43.913rem + -19.5652vw, 40rem);}"
			},
			"core/group": {
				"css": " & .is-nowrap.is-layout-flex.is-style-baseline { align-items: baseline !important; }"
			},
			"core/navigation": {
				"css": "& button {border: 1px solid; padding: 4px;}",
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--bolder)"
				}
			},
			"core/post-content": {
				"css": " & a { text-decoration: underline;} & a:link { color: var(--wp--preset--color--contrast-2); } & a:visited { color: var(--wp--preset--color--contrast-3); } & a.visited { color: var(--wp--preset--color--contrast-3) !important; } & a.unvisited { color: var(--wp--preset--color--contrast-2); }"
			},
			"core/post-excerpt": {
				"css": " & .wp-block-post-excerpt__more-link {font-weight: var(--wp--custom--font-weight--bolder);} & .wp-block-post-excerpt__more-link:hover {text-decoration: none;}"
			},
			"core/query-pagination-next": {
				"css": "background-color: var(--wp--preset--color--contrast-2); color: var(--wp--preset--color--base); padding: var(--wp--preset--spacing--050);"
			},
			"core/query-pagination-previous": {
				"css": "background-color: var(--wp--preset--color--contrast-2); color: var(--wp--preset--color--base); padding: var(--wp--preset--spacing--050);"
			},
			"core/search": {
				"css": " & .wp-block-search__input {font-family: var(--wp--preset--font-family--inter); font-size: var(--wp--preset--font-size--medium); border-color: var(--wp--preset--color--contrast); border-width: var(--wp--custom--border--thin); background-color: var(--wp--preset--color--base-2); padding: var(--wp--preset--spacing--10); }; & .wp-block-search__input::placeholder {color: var(--wp--preset--color--contrast); opacity: 1; font-family: var(--wp--preset--font-family--inter); font-size: var(--wp--preset--font-size--medium); };"
			},
			"core/separator": {
				"css": "border: 2px solid; color: var(--wp--preset--color--contrast-2); &.is-style-dots:before {content: 'Â·';}"
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
